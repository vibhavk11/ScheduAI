{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Debug API",
      "type": "process",
      "command": "dotnet",
      "args": ["run", "--project", "${workspaceFolder}/WebAPI/WebAPI.csproj"],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "presentation": {
        "reveal": "always",
        "panel": "shared",
        "clear": true
      },
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}
